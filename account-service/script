CREATE SEQUENCE account_seq START WITH 1 INCREMENT BY 1;

CREATE TABLE accounts (
    id NUMBER PRIMARY KEY,
    account_number VARCHAR2(255) UNIQUE NOT NULL,
    customer_id VARCHAR2(255) NOT NULL,
    account_type VARCHAR2(50),
    balance NUMBER(19, 4) NOT NULL,
    status VARCHAR2(50)
);
CREATE TABLE account_types (
    account_type_id NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    type_name VARCHAR2(50) UNIQUE NOT NULL,
    description VARCHAR2(255),
    interest_rate NUMBER(5, 2) DEFAULT 0 NOT NULL,
    min_balance NUMBER(15, 2) DEFAULT 0 NOT NULL,
    max_withdrawal_limit NUMBER(15, 2)
);


